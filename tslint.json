{
  "defaultSeverity": "error",
  "extends": ["tslint:latest"],
  "jsRules": {},
  "rules": {
    "triple-equals": true,
    "class-name": true,
    "ordered-imports": true,
    "semicolon": true,
    "radix": true,
    "no-import-side-effect": true,
    "no-var-keyword": true,
    "no-console": false
  },
  "rulesDirectory": ["node_modules/tslint-eslint-rules/dist/rules"]
}
