{
    "parser": "typescript",
    "semi": true,
    "singleQuote": false,
    "trailingComma": "none",
    "arrowParens": "always",
    "endOfLine": "auto",
    "useTabs": false,
    "printWidth": 120,
    "jsxSingleQuote": false,
    "bracketSpacing": true,
    "htmlWhitespaceSensitivity": "css",
    "jsxBracketSameLine": false,
    "proseWrap": "preserve"
}